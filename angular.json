{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "A-Basics": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/A-Basics",
      "sourceRoot": "projects/A-Basics/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/A-Basics",
            "index": "projects/A-Basics/src/index.html",
            "main": "projects/A-Basics/src/main.ts",
            "polyfills": "projects/A-Basics/src/polyfills.ts",
            "tsConfig": "projects/A-Basics/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/A-Basics/src/favicon.ico",
              "projects/A-Basics/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "projects/A-Basics/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/A-Basics/src/environments/environment.ts",
                  "with": "projects/A-Basics/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "A-Basics:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "A-Basics:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "A-Basics:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/A-Basics/src/test.ts",
            "polyfills": "projects/A-Basics/src/polyfills.ts",
            "tsConfig": "projects/A-Basics/tsconfig.spec.json",
            "karmaConfig": "projects/A-Basics/karma.conf.js",
            "assets": [
              "projects/A-Basics/src/favicon.ico",
              "projects/A-Basics/src/assets"
            ],
            "styles": [
              "projects/A-Basics/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/A-Basics/tsconfig.app.json",
              "projects/A-Basics/tsconfig.spec.json",
              "projects/A-Basics/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/A-Basics/e2e/protractor.conf.js",
            "devServerTarget": "A-Basics:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "A-Basics:serve:production"
            }
          }
        }
      }
    },
